"VertexlitGeneric"
{
	"$basetexture" "models/player/braixen/braixen_ear_tufts"
	<dx90
	{
		 	"$no_draw" 1
	}
	"$bumpmap" "models/player/braixen/braixen_ear_tufts_N"
	"$translucent" 1
	"$phong" 1
	"$phongexponenttexture" "models/player/braixen/braixen_ear_tufts_s"
	"$phongalbedotint" 1
	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6
	"$ambientocclusion" 0
	"$nocull" 1
	"$nolod" 1
	"$yellow" "0"
	"$one" "1"
	// Rim lighting parameters
	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "4"										// Exponent for phong component of rim lighting	
	"$rimlightboost" "2"
	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"spy_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1" "$yellow"
			"resultVar" "$color2"
		}
}

}
